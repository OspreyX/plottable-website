<div id="plottable-header">
  <!-- Nav: Main -->
  <nav class="navbar navbar-default" role="navigation">
    <div class="container-fluid">

      <div>
        <ul class="nav navbar-nav">
          <li><a id="plottable-logo" href="/">Plottable.JS</a></li>
          <li class="active"><a href="/docs/modules/plottable.html">API</a></li>
          <li><a href="/components/">Components</a></li>
          <li><a href="/tutorials/">Tutorials</a></li>
          <li><a href="/examples/">Examples</a></li>
          <li><a href="/chartographer/">Chartographer</a></li>
        </ul>
      </div>

    </div><!-- /.container-fluid -->
